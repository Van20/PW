        android:theme="@style/Theme.AppCompat.Light">
        <activity
            android:name=".SignUp_screen.SignUpActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".Login_screen.LoginActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".Forgot_screen.ResetPasswordActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".Beranda_screen.MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".Splash_screen.SplashActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".Welcome_screen.WelcomeActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Uang_screen.DetailReportActivity" />
        <activity android:name=".Uang_screen.SettingUp" />

        <activity
            android:name=".Promo_screen.Blanja_screen.BlanjaActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
